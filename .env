# Copy this file to `.env` and fill in real credentials.

# Use direnv or your shell of choice to load these variables locally.

# --------------------------------------

# Redshift / Postgres connection string

# Format: redshift+psql://user\:password\@host:5439/database

REDSHIFT\_URL=

# AWS region for Glue Catalog, S3 buckets, Lambda, etc.

AWS\_REGION=us-east-1

# Segment Config API PAT with `config` and `reverse-etl` scopes

SEGMENT\_TOKEN=

# Segment Space ID (Dev by default)

SEGMENT\_SPACE\_ID=
